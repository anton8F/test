<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>TITLE</th>
        </tr>
    </thead>
    <tbody>
        <% for (var i = 0; i < posts.length; i++) { %>
            <tr>
                <td><%= posts[i].id %></td>
                <td><a href="/posts/<%= posts[i].id %>"><%= posts[i].title %></a></td>
            </tr>
        <% } %>
    </tbody>
</table>